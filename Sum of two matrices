n,m=map(int,input().split())
a=[]
b=[]
for i in range(n):
    x=list(map(int,input().split()))
    a.append(x)
for j in range(n):
    x=list(map(int,input().split()))
    b.append(x)
for i in range(n):
    for j in range(m):
        a[i][j]=a[i][j]+b[i][j]
        print(a[i][j],end=" ")
    print()
